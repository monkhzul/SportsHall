generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlserver"
  url      = env("DATABASE_URL")
}

model Hall {
  ID   Int     @id(map: "PK_Hall") @default(autoincrement())
  Name String? @db.NVarChar(50)
}

model Time {
  ID   Int  @id(map: "PK_Time") @default(autoincrement())
  time Int?
}
